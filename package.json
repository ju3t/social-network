{
  "name": "social-network",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier src/**/*.{js,jsx} --write",
    "eslint": "eslint src/**/*.{js,jsx}",
    "eslint:fix": "eslint src/**/*.{js,jsx} --fix",
    "eslint:fix:layout": "eslint  src/**/*.{js,jsx} --fix --fix-type layout",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "storybook": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@csstools/normalize.css": "^11.0.1",
    "@reduxjs/toolkit": "^1.4.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-slick": "^0.23.4",
    "@types/react-swipe": "^6.0.0",
    "@types/styled-components": "^5.1.3",
    "@types/swiper": "^5.4.0",
    "antd": "^4.4.3",
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "formik": "^2.1.5",
    "gh-pages": "^3.1.0",
    "lodash": "^4.17.19",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-elastic-carousel": "^0.6.7",
    "react-modal-image": "^2.5.0",
    "react-palette": "^1.0.2",
    "react-player": "^2.6.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-scroll": "^1.8.0",
    "react-scrollbars-custom": "^4.0.25",
    "react-slick": "^0.27.4",
    "react-smooth-collapse": "^2.1.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.1.1",
    "swiper": "^6.1.1",
    "yup": "^0.29.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ]
  }
}
